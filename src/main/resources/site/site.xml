<?xml version="1.0" encoding="UTF-8"?>
<site>
  <config>
    <input name="searchResultMax" type="Long">
      <label>Search result number maximum</label>
      <occurrences minimum="1" maximum="1"/>
      <default>50</default>
    </input>
  </config>

  <mappings>
    <mapping controller="/site/pages/image/image.js">
      <pattern>/image</pattern>
    </mapping>
    <mapping controller="/lib/asset.js">
      <pattern>/service-worker.js</pattern>
    </mapping>
    <mapping controller="/lib/asset.js">
      <pattern>/manifest.json</pattern>
    </mapping>
    <mapping controller="/lib/asset.js">
      <pattern>/browserconfig.xml</pattern>
    </mapping>
    <mapping controller="/lib/asset.js">
      <pattern>/js/.*\.js</pattern>
    </mapping>
    <mapping controller="/lib/asset.js">
      <pattern>/css/.*\.css</pattern>
    </mapping>
    <mapping controller="/lib/asset.js">
      <pattern>/img/.*</pattern>
    </mapping>
    <mapping controller="/lib/asset.js">
      <pattern>/fonts/.*</pattern>
    </mapping>
  </mappings>
</site>
